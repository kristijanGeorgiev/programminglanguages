package student;

public class student {
      String name;
      int indexno;
      int age;
      student()
      {
    	  name="";
    	  indexno=0;
    	  age=0;
      }
      student(String name, int indexno, int age)
      {
    	  this.name=name;
    	  this.indexno=indexno;
    	  this.age=age;
      }
      public String get_name()
      {
    	  return name;
      }
      public void set_name(String name)
      {
    	  this.name=name;
      }
      public int get_indexno()
      {
    	  return indexno;
      }
      public void set_indexno(int indexno)
      {
    	  this.indexno=indexno;
      }
      public int get_age()
      {
    	  return age;
      }
      public void set_age(int age)
      {
    	  this.age=age;
      }
      public void printvalues(student s)
      {
    	  System.out.println(name);
    	  System.out.println(indexno);
    	  System.out.println(age);
      }
public static void main(String[] args)
{
	boolean isyoungerthan25 = false;
	boolean isolderthan25 = false;
	boolean youngerthan25 = true;
	student s1 = new student("Peter Gabriel",6208,28);
	student s2 = new student("Alison Ferguson",2763,27);
	student s3 = new student("Sebastian Pullman",2390,20);
	student s4 = new student("Lily North",8644,25);
	student s5 = new student("Paula Gill",6690,19);
	System.out.println("Print all students: ");
	System.out.println("Student name: " + s1.name + " Student index number: " + s1.indexno + " Student age: " + s1.age);
	System.out.println("Student name: " + s2.name + " Student index number: " + s2.indexno + " Student age: " + s2.age);
	System.out.println("Student name: " + s3.name + " Student index number: " + s3.indexno + " Student age: " + s3.age);
	System.out.println("Student name: " + s4.name + " Student index number: " + s4.indexno + " Student age: " + s4.age);
	System.out.println("Student name: " + s5.name + " Student index number: " + s5.indexno + " Student age: " + s5.age);
	System.out.println("Print all students with name that starts with P: ");
	System.out.println("Student name: " + s1.name + " Student index number: " + s1.indexno + " Student age: " + s1.age);
	System.out.println("Student name: " + s5.name + " Student index number: " + s5.indexno + " Student age: " + s5.age);
	System.out.println("Are all student younger than 25? " + isyoungerthan25);
	System.out.println("Are all student older than 25? " + isolderthan25);
	System.out.println("Is there are a student younger than 25? " + youngerthan25);
  }
}
